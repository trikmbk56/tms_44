<h1><%= t "admin.course.add_admin" %></h1>
<br>
<div class="row">
  <div class="col-md-12 col-md-offset-0">
    <%= form_for @course, url: admin_course_path(@course) do |f| %>
      <%= render "list_admins", f: f %>
      <%= f.submit t("form.submit"),
        class: "btn btn-primary btn-lg col-md-2 col-md-offset-5" %>
    <% end %>
  </div>
</div>
